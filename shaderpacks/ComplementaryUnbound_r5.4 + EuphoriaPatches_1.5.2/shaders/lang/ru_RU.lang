value.BLOCK_REFLECT_QUALITY.0=§aВыкл
value.BLOCK_REFLECT_QUALITY.1=§eНизкие
value.BLOCK_REFLECT_QUALITY.2=§cСредние
value.BLOCK_REFLECT_QUALITY.3=§cВысокие

